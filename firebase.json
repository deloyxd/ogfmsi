{
  "hosting": {
    "site": "fitworxgymph",
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/admin",
        "destination": "/src/html/admin/index.html"
      },
      {
        "source": "/login",
        "destination": "/src/html/login/index.html"
      },
      {
        "source": "/dashboard",
        "destination": "/src/html/dashboard/index.html"
      },
      {
        "source": "/about",
        "destination": "/src/html/about/index.html"
      },
      {
        "source": "/privacy",
        "destination": "/src/html/privacy/index.html"
      },
      {
        "source": "/terms",
        "destination": "/src/html/terms/index.html"
      }
    ]
  }
}
